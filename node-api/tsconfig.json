{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist"
  },
  "exclude": [
    "./app",
    "./model",
    "./common",
    "./electron"
  ]
}